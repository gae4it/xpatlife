---
import favIcon from '~/assets/favicons/streamline-color-earth-airplane-flat.ico';
import favIconSvg from '~/assets/favicons/streamline-color-earth-airplane-flat.svg';
import appleTouchIcon from '~/assets/favicons/streamline-color--earth-airplane-flat.png';
---

<link rel="shortcut icon" href={favIcon} />
<link rel="icon" type="image/svg+xml" href={favIconSvg.src} />
<link rel="mask-icon" href={favIconSvg.src} color="#8D46E7" />
<link rel="apple-touch-icon" sizes="180x180" href={appleTouchIcon.src} />
